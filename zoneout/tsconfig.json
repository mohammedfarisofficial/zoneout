{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
   "baseUrl": "." ,
   "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@helper/*": [
        "src/helper/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@hoc/*": [
        "src/hoc/*"
      ],
   },
}
}
